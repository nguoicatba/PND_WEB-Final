

@{
    ViewData["Title"] = "Not Found";
    Layout =null; 
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Bootstrap Admin App + jQuery">
    <meta name="keywords" content="app, responsive, jquery, bootstrap, dashboard, admin">
    <title>Angle - Bootstrap Admin Template</title>
    <!-- =============== VENDOR STYLES ===============-->
    <!-- FONT AWESOME-->
    <link rel="stylesheet" href="~/app/vendor/font-awesome/css/font-awesome.css">
    <!-- SIMPLE LINE ICONS-->
    <link rel="stylesheet" href="~/app/vendor/simple-line-icons/css/simple-line-icons.css">
    <!-- =============== BOOTSTRAP STYLES ===============-->
    <link rel="stylesheet" href="~/app/css/bootstrap.css" id="bscss">
    <!-- =============== APP STYLES ===============-->
    <link rel="stylesheet" href="~/app/css/app.css" id="maincss">
</head>


<body>

<div class="wrapper">
    <div class="abs-center wd-xl">
        <!-- START card-->
        <div class="text-center mb-4">
            <div class="text-lg mb-3">404</div>
            <p class="lead m-0">We couldn't find this page.</p>
            <p>The page you are looking for does not exists.</p>
        </div>
      
        <ul class="list-inline text-center text-sm mb-4">
            <li class="list-inline-item">
                <a class="text-muted" asp-controller="Home" asp-action="Index">Home</a>
            </li>
            <li class="text-muted list-inline-item">|</li>
            <li class="list-inline-item">
                <a class="text-muted" asp-controller="Action" asp-action="Login">Login</a>
            </li>
         
        </ul>
        <div class="p-3 text-center">
            <span class="mr-2">&copy;</span>
            <span>2025</span>
            <span class="mr-2">-</span>
            <span>PND</span>
            <br>
            <span>More than logistics</span>
        </div>
    </div>
</div>

<!-- =============== VENDOR SCRIPTS ===============-->
<!-- MODERNIZR-->
<script src="~/app/vendor/modernizr/modernizr.custom.js"></script>
<!-- JQUERY-->
<script src="~/app/vendor/jquery/dist/jquery.js"></script>
<!-- BOOTSTRAP-->
<script src="~/app/vendor/bootstrap/dist/js/bootstrap.js"></script>
<!-- STORAGE API-->
<script src="~/app/vendor/js-storage/js.storage.js"></script>
<!-- PARSLEY-->
<script src="~/app/vendor/parsleyjs/dist/parsley.js"></script>
<!-- =============== APP SCRIPTS ===============-->
<script src="~/app/js/app.js"></script>
